<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>

  <VBox fx:id="vbResult" alignment="TOP_CENTER" spacing="30.0" prefHeight="400.0" prefWidth="600.0"  xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.biosoft.fxvocabtrainer.ResultPaneController">
     <children>
        <Label fx:id="lblTotal" text="Label">
           <font>
              <Font size="30.0" />
           </font>
        </Label>
        <Label fx:id="lblFailure" text="Label">
           <font>
              <Font size="30.0" />
           </font>
        </Label>
        <Button fx:id="btnDone" mnemonicParsing="false" defaultButton="true" prefWidth="130.0" text="Ok">
           <font>
              <Font size="25.0" />
           </font>
        </Button>
     </children>
     <padding>
        <Insets top="30.0" />
     </padding>
  </VBox>
